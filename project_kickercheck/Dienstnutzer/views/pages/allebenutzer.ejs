<!DOCTYPE html>
<html>
<head>
    <% include ../partials/head %>
</head>
<body class="container">
    <header>
        <% include ../partials/header %>
    </header>

    <main>
        <div class="row">
            <div class="col-sm-12">
            
                <div class="jumbotron">
                    <h1>Liste aller Benutzer</h1>
                   
        <br />
        
        <div class="list-group">
       
        <% benutzerAll.forEach(function(benutzer) { %>
	  <a href="/Benutzer/<%= benutzer.id %>" class="list-group-item"><h4 class="list-group-item-heading">Benutzer <%= benutzer.id %></h4>
	  <p class="list-group-item-text">Name: <%= benutzer.Name %></p>
	   <p class="list-group-item-text">Alter: <%= benutzer.Alter %></p>
	    </a>
   <% });  %>
</div>
      
                </div>
                
            </div>
           
        </div>
    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>
    
</body>
</html>