<!DOCTYPE html>
<html>
<head>
    <% include ../partials/head %>
</head>
<body class="container">
    <header>
        <% include ../partials/header %>
    </header>

    <main>
        <div class="row">
            <div class="col-sm-12">
            
                <div class="jumbotron">
                    <h1>Liste aller Austragungsorte</h1>
                   
        <br />
        
        <div class="list-group">
       
        <% austragungsorte.forEach(function(austragungsort) { %>
	  <a href="/Austragungsort/<%= austragungsort.id %>" class="list-group-item"> <h4 class="list-group-item-heading"><%= austragungsort.id %></h4>
	  <p class="list-group-item-text">Name: <%= austragungsort.Name %></p>
	   <p class="list-group-item-text">Adresse: <%= austragungsort.Adresse %></p>
	    </a>
   <% });  %>
</div>
      
                </div>
                
            </div>
           
        </div>
    </main>

    <footer>
        <% include ../partials/footer %>
    </footer>
    
</body>
</html>